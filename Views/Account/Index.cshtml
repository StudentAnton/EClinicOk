@model List<UserIndexViewModel>

<h2>Users</h2>

<a class="link-primary" asp-area="" asp-controller="Account" asp-action="Create">Create user</a>

<ul class="list-group">
@foreach (var user in Model)
{
    <li class="list-group-item">@user.FullName @user.Email @user.UserName @user.PhoneNumber @user.Role</li>
}
</ul>